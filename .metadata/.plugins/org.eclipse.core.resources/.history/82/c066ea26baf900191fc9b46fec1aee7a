package modelos;

public class Clientes {

	private int numcli;
	private String nombre;
	private String pobla;
	
	Clientes(){
		
	}
	
	Clientes(int numcli, String nombre, String pobla){
		this.numcli = numcli;
		this.nombre = nombre;
		this.pobla = pobla;
	}

	public int getNumcli() {
		return numcli;
	}

	public void setNumcli(int numcli) {
		this.numcli = numcli;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public String getPobla() {
		return pobla;
	}

	public void setPobla(String pobla) {
		this.pobla = pobla;
	}
	
	
}
